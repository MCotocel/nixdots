bat=/sys/class/power_supply/BAT0/
per="$(cat "$bat/capacity")"

[ $(cat "$bat/status") = Charging ] && echo "󰂄" && exit

if [ "$per" -gt "90" ]; then
	icon="󰁹"
elif [ "$per" -gt "70" ]; then
	icon="󰂀"
elif [ "$per" -gt "50" ]; then
	icon="󰁾"
elif [ "$per" -gt "30" ]; then
	icon="󰁼"
elif [ "$per" -gt "10" ]; then
	icon="󰁺"
fi
echo "$icon"